{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "panels": [],
      "title": "System Overview",
      "type": "row"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 80 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 1 },
      "id": 2,
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "orientation": "auto"
      },
      "pluginVersion": "7.5.0",
      "targets": [
        {
          "expr": "sum(rate(admission_requests_total[1m]))",
          "legendFormat": "Total RPS",
          "refId": "A"
        }
      ],
      "title": "Global Request Rate",
      "type": "stat"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 8, "w": 16, "x": 8, "y": 1 },
      "id": 3,
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(admission_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "Admission P99",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(admission_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "Admission P95",
          "refId": "B"
        }
      ],
      "title": "Admission Latency",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "id": 4,
      "panels": [],
      "title": "Admission & Policy",
      "type": "row"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 10 },
      "id": 5,
      "targets": [
        {
          "expr": "rate(admission_requests_total{status!=\"OK\"}[1m])",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "title": "Errors by Status",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 10 },
      "id": 6,
      "targets": [
        {
          "expr": "sum(rate(admission_policy_denied_total[1m])) by (reason)",
          "legendFormat": "{{reason}}",
          "refId": "A"
        }
      ],
      "title": "Rejection Reasons",
      "type": "piechart"
    },
    {
      "collapsed": false,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "id": 7,
      "panels": [],
      "title": "Controller Health (JVM)",
      "type": "row"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 19 },
      "id": 8,
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"controller\"}[1m])) by (uri)",
          "legendFormat": "{{uri}}",
          "refId": "A"
        }
      ],
      "title": "Controller Traffic by Endpoint",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 19 },
      "id": 9,
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{job=\"controller\", area=\"heap\"}",
          "legendFormat": "Heap Used",
          "refId": "A"
        }
      ],
      "title": "JVM Heap Usage",
      "type": "timeseries"
    }
  ],
  "schemaVersion": 27,
  "style": "dark",
  "tags": ["loadtest", "platform"],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "label": "Datasource",
        "current": {},
        "refresh": 1
      },
      {
        "name": "job",
        "type": "query",
        "datasource": "${DS_PROMETHEUS}",
        "query": "label_values(up, job)",
        "label": "Job",
        "allValue": null,
        "includeAll": true,
        "multi": true,
        "refresh": 1
      }
    ]
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
  },
  "timezone": "browser",
  "title": "LoadTest Platform - Professional",
  "uid": "loadtest-overview",
  "version": 1
